<table>
	<th>Name</th>
	<th>Age</th>
	<th>Birthdate</th>
	<th>Color</th>
	<th>Sex</th>
		<%@cats.each do |cat|%>
			<tr>
				<td><%=cat.name%></td>
				<td><%=cat.age%></td>
				<td><%=cat.birthdate%></td>
				<td><%=cat.color%></td>
				<td><%=cat.sex%></td>
			</tr>
		<%end%>
</table>

<form action="http://localhost:3000/session" method="post">
	<input type="hidden" value="delete" name="_method">
	<input type="submit" value="logout">
</form>
